{
  "name": "@lincy/eslint-config",
  "type": "module",
  "version": "5.0.0",
  "packageManager": "pnpm@9.1.0",
  "description": "LinCenYing's ESLint config",
  "author": "LinCenYing <lincenying@gmail.com> (https://github.com/lincenying/)",
  "license": "MIT",
  "homepage": "https://github.com/lincenying/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "watch": "tsup --format esm,cjs --watch",
    "postpublish": "simple-open-url https://npmmirror.com/package/@lincy/eslint-config",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish -r --access public",
    "test": "vitest",
    "typegen": "esno scripts/typegen.ts",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "eslint . --flag unstable_ts_config --fix",
    "lint:ts": "tsc --noEmit",
    "prepare": "npx simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^7.33.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=9.5.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "^0.4.1",
    "@clack/prompts": "^0.7.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@stylistic/eslint-plugin": "2.7.1",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "@vitest/eslint-plugin": "^1.1.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-flat-config-utils": "^0.3.1",
    "eslint-merge-processors": "^0.1.0",
    "eslint-parser-plain": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.6",
    "eslint-plugin-import-x": "^4.1.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^3.3.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-toml": "^0.11.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^15.9.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@eslint-react/eslint-plugin": "^1.12.3",
    "@eslint-types/jsdoc": "48.2.2",
    "@eslint-types/typescript-eslint": "^7.5.0",
    "@eslint-types/unicorn": "^52.0.0",
    "@eslint/config-inspector": "^0.5.4",
    "@lincy/eslint-config": "workspace:*",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin-migrate": "^2.7.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.5.1",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-plugin": "^0.62.3",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-ts-patch": "9.8.0-1",
    "eslint-typegen": "^0.3.1",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "simple-open-url": "^3.0.1",
    "sucrase": "^3.35.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.5",
    "vue": "^3.4.38"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "prettier"
      ]
    }
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.4.0",
    "@typescript-eslint/utils": "^8.3.0",
    "eslint": "^9.9.1",
    "tsx": "^4.18.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix --flag unstable_ts_config"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
