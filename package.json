{
  "name": "@lincy/eslint-config",
  "type": "module",
  "version": "5.4.2",
  "packageManager": "pnpm@10.6.1",
  "description": "LinCenYing's ESLint config",
  "author": "LinCenYing <lincenying@gmail.com> (https://github.com/lincenying/)",
  "license": "MIT",
  "homepage": "https://github.com/lincenying/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "watch": "tsup --format esm,cjs --watch",
    "postpublish": "simple-open-url https://npmmirror.com/package/@lincy/eslint-config",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish -r --access public",
    "test": "vitest",
    "typegen": "tsx scripts/typegen.ts",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "tsc --noEmit",
    "prepare": "npx simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.15.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=9.5.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.10.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/markdown": "^6.3.0",
    "@stylistic/eslint-plugin": "4.0.0-beta.1",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitest/eslint-plugin": "^1.1.36",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-parser-plain": "^0.1.1",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-pnpm": "^0.1.2",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^10.1.1",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/ni": "^23.3.1",
    "@eslint-react/eslint-plugin": "^1.31.0",
    "@eslint-types/jsdoc": "48.2.2",
    "@eslint-types/typescript-eslint": "^7.5.0",
    "@eslint-types/unicorn": "^52.0.0",
    "@eslint/config-inspector": "^1.0.2",
    "@lincy/eslint-config": "workspace:*",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin-migrate": "^4.2.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@unocss/eslint-plugin": "^66.1.0-beta.3",
    "bumpp": "^10.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-ts-patch": "9.8.0-1",
    "eslint-typegen": "^2.0.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "simple-git-hooks": "^2.11.1",
    "simple-open-url": "^3.0.1",
    "sucrase": "^3.35.0",
    "tinyglobby": "^0.2.12",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.8",
    "vue": "^3.5.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "prettier"
      ]
    }
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.4.1",
    "@typescript-eslint/utils": "^8.26.0",
    "eslint": "^9.22.0",
    "tsx": "^4.19.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
